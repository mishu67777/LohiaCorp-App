{
  "name": "LohiaCorp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "npx react-native run-android",
    "ios": "npx react-native run-ios",
    "lint": "eslint .",
    "assets": "npx react-native-asset",
    "pod": "cd ios && pod install --repo-update && cd ..",
    "delete": "cd ios && pod deintegrate && pod install --repo-update && cd ..",
    "start": "react-native start -- --reset-cache",
    "var": "npx react-native run-android --mode release",
    "rel": "cd android && ./gradlew assembleRelease && cd ..",
    "bundle": "cd android && ./gradlew bundleRelease && cd ..",
    "clean": "cd android && ./gradlew clean && cd ..",
    "crel": "cd android && ./gradlew clean && ./gradlew assembleRelease && cd ..",
    "build": "cd android && ./gradlew clean && ./gradlew bundleRelease && cd ..",
    "test": "jest"
  },
  "dependencies": {
    "@invertase/react-native-apple-authentication": "^2.4.0",
    "@react-native-async-storage/async-storage": "^2.0.0",
    "@react-native-google-signin/google-signin": "^13.0.1",
    "@react-native-picker/picker": "^2.8.0",
    "@react-navigation/drawer": "^6.7.2",
    "@react-navigation/native": "^6.1.18",
    "@react-navigation/native-stack": "^6.11.0",
    "axios": "^1.7.7",
    "react": "18.3.1",
    "react-native": "0.75.2",
    "react-native-gesture-handler": "^2.19.0",
    "react-native-image-picker": "^7.1.2",
    "react-native-math-view": "^3.9.5",
    "react-native-reanimated": "^3.15.1",
    "react-native-render-html": "^6.3.4",
    "react-native-safe-area-context": "^4.10.9",
    "react-native-screens": "^3.34.0",
    "react-native-snap-carousel": "^4.0.0-beta.6",
    "react-native-splash-screen": "^3.3.0",
    "react-native-svg": "^15.7.1",
    "react-native-toast-message": "^2.2.0",
    "react-native-vector-icons": "^10.1.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/babel-preset": "0.75.2",
    "@react-native/eslint-config": "0.75.2",
    "@react-native/metro-config": "0.75.2",
    "@react-native/typescript-config": "0.75.2",
    "@types/react": "^18.2.6",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.6.3",
    "eslint": "^8.19.0",
    "jest": "^29.6.3",
    "prettier": "2.8.8",
    "react-test-renderer": "18.3.1",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=18"
  }
}
